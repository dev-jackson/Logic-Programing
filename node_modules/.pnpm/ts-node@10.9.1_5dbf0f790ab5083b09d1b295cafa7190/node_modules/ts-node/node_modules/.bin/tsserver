#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jackson/Documents/Learning/LogicPrograming/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/bin/node_modules:/home/jackson/Documents/Learning/LogicPrograming/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/node_modules:/home/jackson/Documents/Learning/LogicPrograming/node_modules/.pnpm/typescript@4.8.4/node_modules:/home/jackson/Documents/Learning/LogicPrograming/node_modules/.pnpm/node_modules:/home/jackson/Documents/Learning/LogicPrograming/node_modules:/home/jackson/Documents/Learning/node_modules:/home/jackson/Documents/node_modules:/home/jackson/node_modules:/home/node_modules:/node_modules:/home/jackson/Documents/Learning/LogicPrograming/node_modules/.pnpm/ts-node@10.9.1_5dbf0f790ab5083b09d1b295cafa7190/node_modules/ts-node/node_modules:/home/jackson/Documents/Learning/LogicPrograming/node_modules/.pnpm/ts-node@10.9.1_5dbf0f790ab5083b09d1b295cafa7190/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jackson/Documents/Learning/LogicPrograming/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/bin/node_modules:/home/jackson/Documents/Learning/LogicPrograming/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/node_modules:/home/jackson/Documents/Learning/LogicPrograming/node_modules/.pnpm/typescript@4.8.4/node_modules:/home/jackson/Documents/Learning/LogicPrograming/node_modules/.pnpm/node_modules:/home/jackson/Documents/Learning/LogicPrograming/node_modules:/home/jackson/Documents/Learning/node_modules:/home/jackson/Documents/node_modules:/home/jackson/node_modules:/home/node_modules:/node_modules:/home/jackson/Documents/Learning/LogicPrograming/node_modules/.pnpm/ts-node@10.9.1_5dbf0f790ab5083b09d1b295cafa7190/node_modules/ts-node/node_modules:/home/jackson/Documents/Learning/LogicPrograming/node_modules/.pnpm/ts-node@10.9.1_5dbf0f790ab5083b09d1b295cafa7190/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.8.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.8.4/node_modules/typescript/bin/tsserver" "$@"
fi
